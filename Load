local function LRCJZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Down:Connect(function()
		local func = loadstring(TextBox.Text)
		func()
	end)
end
coroutine.wrap(LRCJZ_fake_script)()
local function BXFEP_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Down:Connect(function()
		TextBox = ""
	end)
end
coroutine.wrap(BXFEP_fake_script)()
local function TRMYLVL_fake_script() -- Moon.LocalScript 
	local script2 = Instance.new('LocalScript', Moon)

	for i,v in pairs(Frame:GetDescendants()) do
		if v:IsA("TextButton") then
			local clr = v.BackgroundColor3
			v.MouseEnter:Connect(function()
				v.BackgroundColor3 = Color3.fromRGB(115, 1, 255)
			end)
			v.MouseLeave:Connect(function()
				v.BackgroundColor3 = clr
			end)
		end
	end

	Moon = math.random()
	game.UserInputService.InputBegan:Connect(function(key,gpe)
		if gpe then return end
		if key.KeyCode == Enum.KeyCode.End then
			script.Parent.Frame.Visible = not script.Parent.Frame.Visible
		end
	end)
end
coroutine.wrap(TRMYLVL_fake_script)()
